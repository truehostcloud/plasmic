{
  "name": "@plasmicpkgs/plasmic-wordpress",
  "version": "0.0.129",
  "description": "Plasmic Wordpress components.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/plasmic-wordpress.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "TEST_CWD=`pwd` yarn --cwd=../.. test --passWithNoTests",
    "lint": "tsdx lint",
    "prepublishOnly": "npm run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@plasmicapp/host": ">=1.0.0",
    "@plasmicapp/query": ">=0.1.0",
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/plasmic-wordpress.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/plasmic-wordpress.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@plasmicapp/host": "1.0.208",
    "@plasmicapp/query": "0.1.79",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "husky": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@types/dlv": "^1.1.2",
    "dlv": "^1.1.3"
  }
}
