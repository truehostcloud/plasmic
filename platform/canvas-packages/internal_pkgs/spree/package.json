  "devDependencies": {
    "@plasmicapp/host": "1.0.207",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/debounce": "^1.2.1",
    "@types/js-cookie": "^3.0.5",
    "@types/node": "^17.0.8",
    "@types/react": "^18.0.27",
    "next": "^12.0.8",
